{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "serena": {
      "name": "serena",
      "transport": "http",
      "url": "http://localhost:8050/mcp"
    },
    "archon": {
      "name": "archon",
    "transport": "http",
    "url": "http://localhost:8051/mcp"
  }
}
